{
  "name": "Malware Detection Extension",
  "description": "Detect malicious websites",
  "version": "1.0",
  "manifest_version": 3,
  "minimum_chrome_version": "92",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "webRequest","activeTab","scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}